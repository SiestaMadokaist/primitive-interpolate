{
  "name": "tslint-custom-rules-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect=5858 -r ts-node/register node_modules/.bin/jest",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eranshabi/tslint-custom-rules-boilerplate.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "jest": "^23.1.0",
    "mocha": "^6.2.2",
    "ts-jest": "^22.4.6",
    "ts-node": "^8.5.4",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "tslint": "^5.10.0",
    "tsutils": "^3.17.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/**/*.spec.(ts|tsx)"
    ],
    "testEnvironment": "node"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eranshabi/tslint-custom-rules-boilerplate/issues"
  },
  "homepage": "https://github.com/eranshabi/tslint-custom-rules-boilerplate#readme"
}
